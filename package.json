{
  "name": "@cygni/paintbot-client-js",
  "version": "0.0.0",
  "repository": "github:cygni/paintbot-client-js",
  "author": "Max Davidson <max.davidson@cygni.se>",
  "license": "MIT",
  "main": "dist/paintbot-client",
  "module": "dist/paintbot-client.mjs",
  "types": "dist/paintbot-client.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && rollup --config && dts-bundle --name dist/paintbot-client --main .types --outputAsModuleFolder && rimraf .types",
    "format": "prettier --write '**/*.{md,yml,js,json,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project tsconfig.json --format stylish --exclude '**/*.json'",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "dts-bundle": "^0.7.3",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6"
  }
}
